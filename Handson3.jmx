   vars.put(&quot;sSearchStrings&quot;, sSearchStrings.join(&quot;, &quot;))
   // Optionally, print the array for debugging purposes
   log.info(&quot;sSearchStrings: &quot; + sSearchStrings)
} else {
   log.info(&quot;sParamsList is null or empty&quot;)
}</stringProp>
